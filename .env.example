# Database
DATABASE_URL="postgresql://postgres:password@localhost:5432/maac_research"

# Redis (for BullMQ)
REDIS_URL="redis://localhost:6379"

# LLM APIs
ANTHROPIC_API_KEY="sk-ant-..."
OPENAI_API_KEY="sk-..."
OPENROUTER_API_KEY="sk-or-..."
DEEPSEEK_API_KEY="..."

# MIMIC Cognitive Engine - Proprietary System Prompt
# The full MIMIC system prompt. This is proprietary and should not be committed.
# Can also be provided programmatically via MIMICConfig.systemPrompt
# If not set, a placeholder prompt will be used for testing.
MIMIC_SYSTEM_PROMPT=

# Neo4j / Graphiti Memory
NEO4J_URI="bolt://localhost:7687"
NEO4J_USERNAME="neo4j"
NEO4J_PASSWORD="password"
# Graphiti MCP Server (HTTP API for memory operations)
# Maps to graphiti-webhook container port 8000
GRAPHITI_WEBHOOK_URL="http://localhost:8090"

# Application
NODE_ENV="development"
API_PORT=3000
DASHBOARD_PORT=3001
